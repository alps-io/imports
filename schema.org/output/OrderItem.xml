<alps>
 <descriptor id="OrderItem" type="semantic" href="http://alps.io/schema.org/Intangible.xml">
  <doc format="html">
   An order item is a line of an order. It includes the quantity and shipping details of a bought offer.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="orderDelivery" type="semantic">
   <doc format="html">
    The delivery of the parcel related to this order or order item.
   </doc>
  </descriptor>

  <descriptor id="orderItemNumber" type="semantic">
   <doc format="html">
    The identifier of the order item.
   </doc>
  </descriptor>

  <descriptor id="orderItemStatus" type="semantic">
   <doc format="html">
    The current status of the order item.
   </doc>
  </descriptor>

  <descriptor id="orderQuantity" type="semantic">
   <doc format="html">
    The number of the item ordered. If the property is not set, assume the quantity is one.
   </doc>
  </descriptor>

  <descriptor id="orderedItem" type="semantic">
   <doc format="html">
    The item ordered.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

 </descriptor>
</alps>