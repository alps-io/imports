<alps>
 <descriptor id="ParcelDelivery" type="semantic" href="http://alps.io/schema.org/Intangible.xml#Intangible">
  <doc format="html">
   The delivery of a parcel either via the postal service or a commercial service.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor id="deliveryAddress" type="semantic">
   <doc format="html">
    Destination address.
   </doc>
  </descriptor>

  <descriptor id="deliveryStatus" type="semantic">
   <doc format="html">
    New entry added as the package passes through each leg of its journey (from shipment to final delivery).
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor id="expectedArrivalFrom" type="semantic">
   <doc format="html">
    The earliest date the package may arrive.
   </doc>
  </descriptor>

  <descriptor id="expectedArrivalUntil" type="semantic">
   <doc format="html">
    The latest date the package may arrive.
   </doc>
  </descriptor>

  <descriptor id="hasDeliveryMethod" type="semantic">
   <doc format="html">
    Method used for delivery or shipping.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor id="itemShipped" type="semantic">
   <doc format="html">
    Item(s) being shipped.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="originAddress" type="semantic">
   <doc format="html">
    Shipper's address.
   </doc>
  </descriptor>

  <descriptor id="partOfOrder" type="semantic">
   <doc format="html">
    The overall order the items in this delivery were included in.
   </doc>
  </descriptor>

  <descriptor id="provider" type="semantic">
   <doc format="html">
    The service provider, service operator, or service performer; the goods producer. Another party (a seller) may offer those services or goods on behalf of the provider. A provider may also serve as the seller.
   </doc>
  </descriptor>

  <descriptor id="trackingNumber" type="semantic">
   <doc format="html">
    Shipper tracking number.
   </doc>
  </descriptor>

  <descriptor id="trackingUrl" type="safe" rt="http://alps.io/schema.org/URL.xml">
   <doc format="html">
    Shipper tracking number.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

 </descriptor>
</alps>