  <descriptor href="http://alps.io/schema.org/MedicalEntity#guideline"/>

  <descriptor href="http://alps.io/schema.org/Thing#image"/>

  <descriptor id="increasesRiskOf" type="semantic" rt="http://alps.io/schema.org/MedicalEntity">
   <doc format="html">
    The condition, complication, etc. influenced by this factor.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#medicineSystem"/>

  <descriptor href="http://alps.io/schema.org/Thing#name"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#recognizingAuthority"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#relevantSpecialty"/>

