<alps>
 <descriptor id="InfectiousDisease" type="semantic" href="http://alps.io/schema.org/MedicalCondition">
  <doc format="html">
   An infectious disease is a clinically evident human disease resulting from the presence of pathogenic microbial agents, like pathogenic viruses, pathogenic bacteria, fungi, protozoa, multicellular parasites, and prions. To be considered an infectious disease, such pathogens are known to be able to cause this disease.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing#additionalType"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#alternateName"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#associatedAnatomy"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#cause"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#code"/>

  <descriptor href="http://alps.io/schema.org/Thing#description"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#differentialDiagnosis"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#epidemiology"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#expectedPrognosis"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#guideline"/>

  <descriptor href="http://alps.io/schema.org/Thing#image"/>

  <descriptor id="infectiousAgent" type="semantic" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    The actual infectious agent, such as a specific bacterium.
   </doc>
  </descriptor>

  <descriptor id="infectiousAgentClass" type="semantic" rt="http://alps.io/schema.org/InfectiousAgentClass">
   <doc format="html">
    The class of infectious agent (bacteria, prion, etc.) that causes the disease.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#medicineSystem"/>

  <descriptor href="http://alps.io/schema.org/Thing#name"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#naturalProgression"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#pathophysiology"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#possibleComplication"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#possibleTreatment"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#primaryPrevention"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#recognizingAuthority"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#relevantSpecialty"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#riskFactor"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#secondaryPrevention"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#signOrSymptom"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#stage"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity#study"/>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#subtype"/>

  <descriptor id="transmissionMethod" type="semantic" rt="http://alps.io/schema.org/Text">
   <doc format="html">
    How the disease spreads, either as a route or vector, for example 'direct contact', 'Aedes aegypti', etc.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalCondition#typicalTest"/>

  <descriptor href="http://alps.io/schema.org/Thing#url"/>

 </descriptor>
</alps>