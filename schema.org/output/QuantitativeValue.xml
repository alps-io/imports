<alps>
 <descriptor id="QuantitativeValue" type="semantic" href="http://alps.io/schema.org/StructuredValue.xml">
  <doc format="html">
   A point value or interval for product characteristics and other purposes.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor id="maxValue" type="semantic">
   <doc format="html">
    The upper of the product characteristic.
   </doc>
  </descriptor>

  <descriptor id="minValue" type="semantic">
   <doc format="html">
    The lower value of the product characteristic.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="unitCode" type="semantic">
   <doc format="html">
    The unit of measurement given using the UN/CEFACT Common Code (3 characters).
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

  <descriptor id="value" type="semantic">
   <doc format="html">
    The value of the product characteristic.
   </doc>
  </descriptor>

  <descriptor id="valueReference" type="semantic">
   <doc format="html">
    A pointer to a secondary value that provides additional information on the original value, e.g. a reference temperature.
   </doc>
  </descriptor>

 </descriptor>
</alps>