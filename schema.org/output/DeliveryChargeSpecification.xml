<alps>
 <descriptor id="DeliveryChargeSpecification" type="semantic" href="http://alps.io/schema.org/PriceSpecification.xml#PriceSpecification">
  <doc format="html">
   The price for the delivery of an offer using a particular delivery method.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor id="appliesToDeliveryMethod" type="semantic">
   <doc format="html">
    The delivery method(s) to which the delivery charge or payment charge specification applies.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#eligibleQuantity"/>

  <descriptor id="eligibleRegion" type="semantic">
   <doc format="html">
    The ISO 3166-1 (ISO 3166-1 alpha-2) or ISO 3166-2 code, or the GeoShape for the geo-political region(s) for which the offer or delivery charge specification is valid.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#eligibleTransactionVolume"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#maxPrice"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#minPrice"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#price"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#priceCurrency"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#validFrom"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#validThrough"/>

  <descriptor href="http://alps.io/schema.org/PriceSpecification.xml#valueAddedTaxIncluded"/>

 </descriptor>
</alps>