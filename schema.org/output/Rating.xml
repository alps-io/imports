<alps>
 <descriptor id="Rating" type="semantic" href="http://alps.io/schema.org/Intangible.xml">
  <doc format="html">
   The rating of the video.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor id="bestRating" type="semantic">
   <doc format="html">
    The highest value allowed in this rating system. If bestRating is omitted, 5 is assumed.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="ratingValue" type="semantic">
   <doc format="html">
    The rating for the content.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

  <descriptor id="worstRating" type="semantic">
   <doc format="html">
    The lowest value allowed in this rating system. If worstRating is omitted, 1 is assumed.
   </doc>
  </descriptor>

 </descriptor>
</alps>