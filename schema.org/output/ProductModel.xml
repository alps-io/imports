<alps>
 <descriptor id="ProductModel" type="semantic" href="http://alps.io/schema.org/Product.xml">
  <doc format="html">
   A datasheet or vendor specification of a product (in the sense of a prototypical description).
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#aggregateRating"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#brand"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#color"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#depth"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#gtin13"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#gtin14"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#gtin8"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#height"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#isAccessoryOrSparePartFor"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#isConsumableFor"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#isRelatedTo"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#isSimilarTo"/>

  <descriptor id="isVariantOf" type="semantic">
   <doc format="html">
    A pointer to a base product from which this product is a variant. It is safe to infer that the variant inherits all product features from the base model, unless defined locally. This is not transitive.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Product.xml#itemCondition"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#logo"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#manufacturer"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#model"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#mpn"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#offers"/>

  <descriptor id="predecessorOf" type="semantic">
   <doc format="html">
    A pointer from a previous, often discontinued variant of the product to its newer variant.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Product.xml#productID"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#releaseDate"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#review"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#reviews"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#sku"/>

  <descriptor id="successorOf" type="semantic">
   <doc format="html">
    A pointer from a newer variant of a product  to its previous, often discontinued predecessor.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#weight"/>

  <descriptor href="http://alps.io/schema.org/Product.xml#width"/>

 </descriptor>
</alps>