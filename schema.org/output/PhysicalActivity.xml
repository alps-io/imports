<alps>
 <descriptor id="PhysicalActivity" type="semantic" href="http://alps.io/schema.org/LifestyleModification.xml#LifestyleModification">
  <doc format="html">
   Any bodily activity that enhances or maintains physical fitness and overall health and wellness. Includes activity that is part of daily living and routine, structured exercise, and exercise prescribed as part of a medical treatment or recovery plan.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/MedicalTherapy.xml#adverseOutcome"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#alternateName"/>

  <descriptor id="associatedAnatomy" type="semantic">
   <doc format="html">
    The anatomy of the underlying organ system or structures associated with this entity.
   </doc>
  </descriptor>

  <descriptor id="category" type="semantic">
   <doc format="html">
    A category for the item. Greater signs or slashes can be used to informally indicate a category hierarchy.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#code"/>

  <descriptor href="http://alps.io/schema.org/MedicalTherapy.xml#contraindication"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/MedicalTherapy.xml#duplicateTherapy"/>

  <descriptor id="epidemiology" type="semantic">
   <doc format="html">
    The characteristics of associated patients, such as age, gender, race etc.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#guideline"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/MedicalTherapy.xml#indication"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#medicineSystem"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="pathophysiology" type="semantic">
   <doc format="html">
    Changes in the normal mechanical, physical, and biochemical functions that are associated with this activity or condition.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#recognizingAuthority"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#relevantSpecialty"/>

  <descriptor href="http://alps.io/schema.org/MedicalTherapy.xml#seriousAdverseOutcome"/>

  <descriptor href="http://alps.io/schema.org/MedicalEntity.xml#study"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

 </descriptor>
</alps>