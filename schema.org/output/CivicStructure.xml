   A public structure, such as a town hall or concert hall.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#address"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#aggregateRating"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#containedIn"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#event"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#events"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#faxNumber"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#geo"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#globalLocationNumber"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

