<alps>
 <descriptor id="LocalBusiness" type="semantic" href="http://alps.io/schema.org/Organization.xml http://alps.io/schema.org/Place.xml">
  <doc format="html">
   A particular physical business or branch of an organization. Examples of LocalBusiness include a restaurant, a particular branch of a restaurant chain, a branch of a bank, a medical practice, a club, a bowling alley, etc.
  </doc>

  <descriptor href="http://alps.io/schema.org/Thing.xml#additionalType"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#address"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#aggregateRating"/>

  <descriptor id="branchOf" type="semantic">
   <doc format="html">
    The larger organization that this local business is a branch of, if any.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Organization.xml#brand"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#contactPoint"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#contactPoints"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#containedIn"/>

  <descriptor id="currenciesAccepted" type="semantic">
   <doc format="html">
    The currency accepted (in <a href='http://en.wikipedia.org/wiki/ISO_4217'>ISO 4217 currency format</a>).
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Thing.xml#description"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#duns"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#email"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#employee"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#employees"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#event"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#events"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#faxNumber"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#founder"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#founders"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#foundingDate"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#geo"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#globalLocationNumber"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#hasPOS"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#image"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#interactionCount"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#isicV4"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#legalName"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#location"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#logo"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#makesOffer"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#map"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#maps"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#member"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#members"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#naics"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#name"/>

  <descriptor id="openingHours" type="semantic">
   <doc format="html">
    The opening hours for a business. Opening hours can be specified as a weekly time range, starting with days, then times per day. Multiple days can be listed with commas ',' separating each day. Day or time ranges are specified using a hyphen '-'.<br />- Days are specified using the following two-letter combinations: <code>Mo</code>, <code>Tu</code>, <code>We</code>, <code>Th</code>, <code>Fr</code>, <code>Sa</code>, <code>Su</code>.<br />- Times are specified using 24:00 time. For example, 3pm is specified as <code>15:00</code>. <br />- Here is an example: <code><time itemprop="openingHours" datetime="Tu,Th 16:00-20:00">Tuesdays and Thursdays 4-8pm</time></code>. <br />- If a business is open 7 days a week, then it can be specified as <code><time itemprop="openingHours" datetime="Mo-Su">Monday through Sunday, all day</time></code>.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Place.xml#openingHoursSpecification"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#owns"/>

  <descriptor id="paymentAccepted" type="semantic">
   <doc format="html">
    Cash, credit card, etc.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Place.xml#photo"/>

  <descriptor href="http://alps.io/schema.org/Place.xml#photos"/>

  <descriptor id="priceRange" type="semantic">
   <doc format="html">
    The price range of the business, for example <code>$$$</code>.
   </doc>
  </descriptor>

  <descriptor href="http://alps.io/schema.org/Organization.xml#review"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#reviews"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#seeks"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#taxID"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#telephone"/>

  <descriptor href="http://alps.io/schema.org/Thing.xml#url"/>

  <descriptor href="http://alps.io/schema.org/Organization.xml#vatID"/>

 </descriptor>
</alps>